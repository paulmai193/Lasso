<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- STORAGE PATH -->
	<bean id="webContextStoragePath" class="java.lang.String" scope="singleton">
		<constructor-arg type="java.lang.String"
			value="" />
	</bean>

	<bean id="avatarStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Users" />
	</bean>
	
	<bean id="bannerStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Banners" />
	</bean>
	
	<bean id="categoryStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Categories" />
	</bean>
	
	<bean id="jobStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Jobs" />
	</bean>
	
	<bean id="portfolioStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Portfolios" />
	</bean>
	
	<bean id="projectStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Projects" />
	</bean>
	
	<bean id="styleStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Styles" />
	</bean>
	
	<bean id="typeStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/Types" />
	</bean>
	
	<bean id="temporaryStoragePath" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="/caches" />
	</bean>
	
	<!-- HTTP HOST -->
	<bean id="httpHost" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="http://lasso.voolatech.com" />
	</bean>
	
	<bean id="httpImageHost" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="http://lasso.voolatech.com/img" />
	</bean>
	
	<!-- FIREBASE PUSH NOTIFICATION -->
	<bean id="firebaseApiKey" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="AIzaSyA4dEJ5zI7dcEjnBSfsJkkmrIvmiY9N3nQ" />
	</bean>
	
	<!-- PAYPAL -->
	<bean id="paypalHost" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="https://api.sandbox.paypal.com/v1" />
	</bean>
	
	<bean id="paypalClientId" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="AQk0nr5BNlW91KqFN_PKkeszaxp1k0T1p35FatoTA3l0bYZtJ3xIn_sSdynRnpxksHBDoPRlASffmrkZ" />
	</bean>
	
	<bean id="paypalClientSecret" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="EIKQLF0bcf9uVZWxq2zlDUxNjOkCryL-hDutX8TfJ5FdzGY0_wKk2byyCrVqmWKIFCyZb2YSWNNGZiX8" />
	</bean>
	
</beans>
